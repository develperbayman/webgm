{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["warn", { "args": "none" }],
        "no-empty-pattern": "off",
        "no-constant-condition": "off"
    }
}
